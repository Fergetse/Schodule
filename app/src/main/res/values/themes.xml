<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Schodule" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color.
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorPrimary">@color/purple_700</item> -->
        <item name="colorPrimaryVariant">@color/lighter_purple</item>
        <item name="colorPrimary">@color/Blanco</item>

        <item name="colorOnPrimary">@color/primaryDark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorAccent">@color/teal_200</item>

        <item name="colorPrimaryDark">@color/primaryDark</item>
        <item name="fontFamily">@font/corbellight</item>
    </style>

    <style name="textStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/corbellight</item>
        <item name="android:textColorSecondary">@color/Blanco</item>
    </style>


    <style name="AppTheme">
        <item name="fontFamily">@font/corbellight</item>
        <item name="colorAccent">@color/teal_200</item>
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryDark">@color/primaryDark</item>
    </style>

    <style name="TableRow">
        <item name="fontFamily">@font/corbellight</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@color/darker</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">70dp</item>
    </style>

    <style name="Cell">
        <item name="fontFamily">@font/corbellight</item>
        <item name="android:textSize">30dp</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:maxWidth">100dp</item>
        <item name="android:text">""</item>
    </style>
    <style name="LeftCell">
        <item name="android:background">@color/colorPrimary</item>
        <item name="fontFamily">@font/corbellight</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>
    <style name="MiddleCell">
        <item name="android:background">@color/colorPrimary</item>
        <item name="fontFamily">@font/corbellight</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>
    <style name="RightCell">
        <item name="android:background">@color/colorPrimary</item>
        <item name="fontFamily">@font/corbellight</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>

    <style name="Theme.Schodule.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="fontFamily">@font/corbellight</item>

        <item name="android:listDivider">@color/colorPrimary</item>
    </style>

    <style name="NumberPickerStyle">
        <item name="colorControlNormal">@color/teal_200</item>
        <item name="android:textColorPrimary">@color/Blanco</item>
        <item name="fontFamily">@font/corbellight</item>
    </style>

    <style name="AlertD" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <item name="android:textSize">15sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/background</item>
        <item name="fontFamily">@font/corbellight</item>
        <item name="android:buttonBarPositiveButtonStyle">@style/PositiveButtonStyle</item>
        <item name="android:buttonBarNegativeButtonStyle">@style/NegativeButtonStyle</item>
        <item name="android:textColor">@color/Blanco</item>
        <item name="android:textColorHint">@color/Blanco</item>
        <item name="colorAccent">@color/Blanco</item>
        <item name="colorPrimary">@color/Blanco</item>
        <item name="colorOnPrimary">@color/Blanco</item>
        <item name="colorPrimaryDark">@color/Blanco</item>
        <item name="colorOnSecondary">@color/Blanco</item>
        <item name="colorPrimaryVariant">@color/Blanco</item>
        <item name="android:textColorTertiary">@color/Blanco</item>
        <item name="android:textColorTertiaryInverse">@color/Blanco</item>
        <item name="android:colorBackground">@color/Blanco</item>
        <item name="android:colorForeground">@color/Blanco</item>
    </style>

    <style name="NegativeButtonStyle" parent="Widget.AppCompat.ButtonBar.AlertDialog">
        <item name="android:textColor">@color/Blanco</item>
    </style>

    <style name="PositiveButtonStyle" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textColor">@color/blanco100</item>
    </style>

    <style name="Theme.Schodule.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Schodule.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    
</resources>